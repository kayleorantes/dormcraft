{
  "name": "mit-dormcraft",
  "version": "1.0.0",
  "description": "A demo application showing how to use Google's Gemini API to create a dorm layout application",
  "main": "dist/dormcraft_tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/dormcraft_tests.js",
    "dev": "ts-node --transpile-only dormcraft_tests.ts",
    "test": "npm run build && node dist/dormcraft_tests.js",
    "test:ai": "npm run build && node -e \"require('./dist/dormcraft_tests.js').testAISuggestion()\"",
    "test:v1": "npm run build && node -e \"require('./dist/dormcraft_tests.js').testValidator1Failure()\"",
    "test:v3": "npm run build && node -e \"require('./dist/dormcraft_tests.js').testValidator3Failure()\""
  },
  "keywords": ["gemini", "ai", "dormcraft", "layout", "typescript"],
  "author": "Student Demo",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
